.air-cell{position:relative;padding:12px 15px;display:flex;background:#fff;align-items:center;line-height:1.5;font-size:14px;overflow:hidden}.air-cell::after{content:'';position:absolute;top:0;left:0;width:200%;height:200%;transform:scale(.5);transform-origin:0 0;pointer-events:none;box-sizing:border-box;border:0 solid #c0ccda;border-bottom-width:1px;left:15px;right:0}.air-cell-last::after{display:none}.air-cell-icon{margin-right:5px}.air-cell-icon:empty{display:none}.air-cell-bd{flex:1}.air-cell-text{line-height:24px;font-size:14px}.air-cell-desc{line-height:1.2;font-size:12px;color:#80848f}.air-cell-ft{position:relative;text-align:right;color:#495060}.air-cell-access .air-cell-ft{padding-right:13px}.air-cell-access .air-cell-ft::after{content:" ";display:inline-block;width:6px;height:6px;position:absolute;top:50%;right:2px;border-width:2px 2px 0 0;border-color:#dddee1;border-style:solid;transform:translateY(-50%) matrix(.71,.71,-.71,.71,0,0)}