.air-stepper{color:#495060}.air-stepper view{display:inline-block;line-height:20px;padding:5px 0;text-align:center;min-width:40px;box-sizing:border-box;vertical-align:middle;font-size:12px;border:1rpx solid #dddee1}.air-stepper-minus{border-right:none!important;border-radius:2px 0 0 2px}.air-stepper-plus{border-left:none!important;border-radius:0 2px 2px 0}.air-stepper-text{border:1rpx solid #dddee1;display:inline-block;text-align:center;vertical-align:middle;height:30px;width:40px;min-height:auto;font-size:12px;line-height:30px}.air-stepper-disabled{border-color:#dddee1;color:#bbbec4;background:#f7f7f7}